                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- the transformations on eservices_szhp

WITH TransformedData AS (
    SELECT
        -- Direct column renames
        CreatedDate AS AppStartDate,
        RequestSubmitDate AS Created,
        CompletionDate AS Modified,
        CreatorTypeAra AS DeviceType,
        AgentName AS BackOfficeEmployee,
        IPAddress AS IPAddress, -- Final name after renames in Power Query
        CurrentStateAra AS StatusSource,
        ServiceNameAr AS ServiceNameSource,
        IDNumber AS NationalID,
        ETransactionId,
 
        -- ApplicationNumber logic: Combine ApplicationID and ETransactionId
        CASE
            WHEN ApplicationID IS NOT NULL THEN
                CONCAT(CAST(ApplicationID AS NVARCHAR(50)), '-', CAST(ETransactionId AS NVARCHAR(50)))
            ELSE
                CAST(ETransactionId AS NVARCHAR(50))
        END AS ApplicationNumber,
 
        -- Static value
        N'برنامج الشيخ زايد للإسكان' AS EntityName,
 
        -- Year and Hour from Created
        DATEPART(YEAR, RequestSubmitDate) AS CreatedYear,
        DATEPART(HOUR, RequestSubmitDate) AS Hour,
 
        -- Trim and standardize ServiceName
        LTRIM(RTRIM(
            CASE
			    WHEN ServiceNameAr = N'تقديم تظلم عن مقدار مبلغ المساعدة السكنية' THEN N'إعادة النظر في قرار المساعدة السكنية'
			    WHEN ServiceNameAr = N'طلب دعم سكني جديد' THEN N'طلب المساعدة السكنية'
			    WHEN ServiceNameAr = N'طلبات الاستلام الابتدائي' THEN N'طلب الإستلام الإبتدائي للمساكن الفردية'
			    WHEN ServiceNameAr = N'تحديث بيانات طلب الدعم السكني' THEN N'تحديث بيانات المساعدة السكنية'
			    WHEN ServiceNameAr = N'طلب تسجيل الموردين' THEN N'خدمة طلب تسجيل مورد جديد'
			    WHEN ServiceNameAr = N'طلبات إسناد الصيانة' THEN N'إسناد الصيانة'
			    WHEN ServiceNameAr = N'طلبات الإيقاف المؤقت' THEN N'إيقاف مؤقت'
			    WHEN ServiceNameAr = N'طلبات الزيارة الميدانية للمشروع' THEN N'الزيارة الميدانية للمشروع'
			    WHEN ServiceNameAr = N'طلبات إنهاء التعاقد' THEN N'طلب إنهاء تعاقد'
			    WHEN ServiceNameAr = N'طلب تجديد المقاولين والاستشاريين' THEN N'طلب تجديد استشاري/مقاول'
			    WHEN ServiceNameAr = N'طلب تسجيل المقاولين والاستشاريين' THEN N'طلب تسجيل جديد لمقاول/طلب تسجيل جديد لاستشاري'
			    WHEN ServiceNameAr = N'طلبات تغيير قيمة العقد' THEN N'طلب تعديل قيمة العقد'
			    WHEN ServiceNameAr = N'طلبات تغيير مقاول' THEN N'طلب تغيير مقاول'
			    WHEN ServiceNameAr = N'طلبات دفعات المشاريع' THEN N'طلب تقديم دفعات المساكن الفردية'
			    WHEN ServiceNameAr = N'طلب رسالة إلى من يهمه الأمر' THEN N'طلب إصدار شهادة لمن يهمه الأمر لخدمات الإسكان'
			    WHEN ServiceNameAr = N'رسالة بالقرار الصادر' THEN N'رسالة الموافقة'
			    WHEN ServiceNameAr = N'طلب استكمال الوثائق والبيانات للمتقدمين' THEN N'طلب استكمال الوثائق والبيانات'
			    WHEN ServiceNameAr = N'طلب زيارة طارئة' THEN N'طلب زيارة طارئة لمسكن'
			    WHEN ServiceNameAr = N'طلب تأجيل اقساط' THEN N'تخفيض أو تأجيل قسط المساعدة السكنية'
			    WHEN ServiceNameAr = N'فتح ملف' THEN N'طلب فتح ملف تنفيذ مساعدة سكنية'
			    WHEN ServiceNameAr = N'طلب خفض قسط' THEN N'تخفيض أو تأجيل قسط المساعدة السكنية'
			    WHEN ServiceNameAr = N'طلب زيارة طارئة للمسكن' THEN N'طلب زيارة طارئة لمسكن'
			    WHEN ServiceNameAr = N'طلب التصرف في مساكن المنح' THEN N'طلب التصرف في مسكن'
			    WHEN ServiceNameAr = N'طلب الغاء طلب مساعدة سكنية' THEN N'طلب الغاء المساعدة السكنية'
			    WHEN ServiceNameAr = N'طلب تمليك المساكن الحكومية' THEN N'تمليك المساكن الحكومية'
			    WHEN ServiceNameAr = N'طلب إعفاء من قرض المساعدة السكنية' THEN N'إعفاء من قرض المساعدة السكنية'
			    WHEN ServiceNameAr = N'طلب إعادة جدولة المتأخرات للمساعدة السكنية' THEN N'إعادة جدولة المتأخرات للمساعدة السكنية'
			    ELSE ServiceNameAr
            END
        )) AS ServiceName,
 
        -- Translate Status from Arabic to standardized values
        LTRIM(RTRIM(
            CASE
			    WHEN CurrentStateAra = N'تم إلغاء طلب المساعدة' THEN 'Rejected'
			    WHEN CurrentStateAra = N'مرفوضة الطلب' THEN 'Rejected'
			    WHEN CurrentStateAra = N'مرتجعة الطلب من اللجنة لطلب معلومات' THEN 'Returned'
			    WHEN CurrentStateAra = N'ملغي لانقضاء المدة المحددة' THEN 'Rejected'
			    WHEN CurrentStateAra = N'أصدر القرار-مرفوضة' THEN 'Rejected'
			    WHEN CurrentStateAra = N'تم إلغاء الطلب' THEN 'Rejected'
			    WHEN CurrentStateAra = N'مرفوض' THEN 'Rejected'
			    WHEN CurrentStateAra = N'تم رفض الطلب' THEN 'Rejected'
			    WHEN CurrentStateAra = N'رفض' THEN 'Rejected'
			    WHEN CurrentStateAra = N'تم رفض التقرير' THEN 'Rejected'
			    WHEN CurrentStateAra = N'تم التدقيق' THEN 'Completed'
			    WHEN CurrentStateAra = N'مكتملة' THEN 'Completed'
			    WHEN CurrentStateAra = N'مكتمل' THEN 'Completed'
			    WHEN CurrentStateAra = N'تمت الموافقة على الطلب، يرجى التواصل مع مصرف الإمارات للتنمية لمزيد من المعلومات' THEN 'Completed'
			    WHEN CurrentStateAra = N'قيد التدقيق' THEN 'InProgress'
			    WHEN CurrentStateAra = N'تم الإنشاء' THEN 'InProgress'
			    WHEN CurrentStateAra = N'تم الإجراء' THEN 'InProgress'
			    WHEN CurrentStateAra = N'اعتماد' THEN 'Completed'
			    WHEN CurrentStateAra = N'اعتمده' THEN 'Completed'
			    WHEN CurrentStateAra = N'أرسل إلى المصرف' THEN 'Completed'
			    WHEN CurrentStateAra = N'تم اعتماد الطلب' THEN 'Completed'
			    WHEN CurrentStateAra = N'تم قبول المقاول' THEN 'Completed'
			    WHEN CurrentStateAra = N'تم إنشاء تقرير' THEN 'Completed'
			    WHEN CurrentStateAra = N'أرسلت الى المالية' THEN 'InProgress'
			    WHEN CurrentStateAra = N'إرجاع' THEN 'Returned'
			    WHEN CurrentStateAra = N'إرجاع إلى المنتفع' THEN 'Returned'
			    WHEN CurrentStateAra = N'جاهز للتدقيق' THEN 'InProgress'
			    WHEN CurrentStateAra = N'مرتجعة من المالية' THEN 'Returned'
			    WHEN CurrentStateAra = N'مرتجعة من قسم التحصيل' THEN 'Returned'
			    WHEN CurrentStateAra = N'Returned إلى المنتفع' THEN 'Returned'
			    WHEN CurrentStateAra = N'إرسال الى المصرف' THEN 'Completed'
			    WHEN CurrentStateAra = N'تم الإعتماد' THEN 'Completed'
			    WHEN CurrentStateAra = N'أصدر القرار-حاصل' THEN 'Completed'
			    WHEN CurrentStateAra = N'تمت الموافقة من قبل العميل' THEN 'Completed'
			    WHEN CurrentStateAra = N'جاهز للزيارة الميدانية' THEN 'InProgress'
			    WHEN CurrentStateAra = N'إرسال إتفاقية إعادة جدولة الأقساط المتأخرة  ' THEN 'InProgress'
			    WHEN CurrentStateAra = N'Rejected الطلب' THEN 'Rejected'
			    WHEN CurrentStateAra = N'Returned الطلب من اللجنة لطلب معلومات' THEN 'Returned'
			    WHEN CurrentStateAra = N'أصدر القرار-Rejected' THEN 'Rejected'
			    WHEN CurrentStateAra = N'جديد' THEN 'InProgress'
			    WHEN CurrentStateAra = N'أصدر القرار-مرفوض' THEN 'Rejected'
			    WHEN CurrentStateAra = N'إرجاع الطلب من اللجنة لطلب معلومات' THEN 'Returned'
			    WHEN CurrentStateAra = N'طلب المساعدة قرار ملغى' THEN 'Rejected'
			    WHEN CurrentStateAra = N'رفض الطلب' THEN 'Rejected'
			    WHEN CurrentStateAra = N'توصية اللجنة' THEN 'InProgress'
			    WHEN CurrentStateAra = N'تم الإلغاء' THEN 'Rejected'
			    WHEN CurrentStateAra = N'قيد الدراسة' THEN 'InProgress'
			    ELSE CurrentStateAra
            END
        )) AS Status,
 
        -- KPI_Days based on ServiceName
        CASE
		    WHEN ServiceNameAr = N'طلب إصدار شهادة لمن يهمه الأمر لخدمات الإسكان' THEN 1  
		    WHEN ServiceNameAr = N'طلب إعادة جدولة متأخرات المساعدة السكنية' THEN 10
		    WHEN ServiceNameAr = N'طلب تخفيض أو تأجيل قسط المساعدة السكنية' THEN 15
		    WHEN ServiceNameAr = N'إعادة النظر في قرار المساعدة السكنية' THEN 10
		    WHEN ServiceNameAr = N'طلب الغاء المساعدة السكنية' THEN 5
		    WHEN ServiceNameAr = N'طلب المساعدة السكنية' THEN 90
		    WHEN ServiceNameAr = N'تحديث بيانات المساعدة السكنية' THEN 1                       
		    WHEN ServiceNameAr = N'طلب زيارة طارئة لمسكن' THEN 7
		    WHEN ServiceNameAr = N'طلب فتح ملف تنفيذ مساعدة سكنية' THEN 1
		    WHEN ServiceNameAr = N'طلب الإستلام الإبتدائي للمساكن الفردية' THEN 9
		    WHEN ServiceNameAr = N'طلب اعفاء من قرض المساعدة السكنية' THEN 25
		    WHEN ServiceNameAr = N'طلب تمليك المساكن الحكومية' THEN 30
		    ELSE 0
        END AS KPI_Days,    -- seham says here that we can ignore that column
 
        -- Nationality logic
        CASE
            WHEN ServiceNameAr &lt;&gt; N'طلب الإستلام الإبتدائي للمساكن الفردية' THEN 'UAE'
            ELSE NULL
        END AS Nationality,
 
        -- IsPmo_Confirmed flag
        CASE
            WHEN ServiceNameAr IN (
		        N'طلب المساعدة السكنية',
		        N'طلب فتح ملف تنفيذ مساعدة سكنية',
		        N'تحديث بيانات المساعدة السكنية',
		        N'إعادة النظر في قرار المساعدة السكنية',
		        N'طلب إصدار شهادة لمن يهمه الأمر لخدمات الإسكان',
		        N'إعفاء من قرض المساعدة السكنية',
		        N'تخفيض أو تأجيل قسط المساعدة السكنية',
		        N'إعادة جدولة المتأخرات للمساعدة السكنية',
		        N'طلب الإستلام الإبتدائي للمساكن الفردية',
		        N'طلب الغاء المساعدة السكنية',
		        N'طلب زيارة طارئة لمسكن',
		        N'طلب التصرف في مسكن'
            ) THEN 'Yes'
            ELSE 'No'
        END AS IsPmo_Confirmed
    FROM
        moeidw.dbo.eservices_szhp
    WHERE
        -- Filter out invalid Statuses
        CurrentStateAra IS NOT NULL
        AND CurrentStateAra &lt;&gt; N'فريق التطوير'
)
SELECT
    AppStartDate,
    ApplicationNumber,
    Created,
    Modified,
    DeviceType,
    BackOfficeEmployee,
    IPAddress,
    CreatedYear,
    Status,
    ServiceName,
    EntityName,
    KPI_Days,
    NationalID,
    Nationality,
    IsPmo_Confirmed,
    Hour
FROM
    TransformedData;</property>
